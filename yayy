    print("executed")
else 
    if _G.killv2 == false then
        loadstring(game:HttpGet('https://raw.githubusercontent.com/BloodyBurns/Hexx/main/Iv%20Admin/Admin%202.lua'))()
        game:GetService("CoreGui"):WaitForChild("IvAdmin CommandBar").Enabled = false
        game:GetService("CoreGui"):WaitForChild("IvAdmin Notifications").Enabled = false
        plr.Character.Head:WaitForChild("Client Bridge v3").Enabled = false
        _G.killv2 = true
    end
end
end
