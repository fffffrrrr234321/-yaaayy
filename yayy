function Dupe(amount)
    if tonumber(amount) or 1 then
    local DropT = {}
    plr.Character.Humanoid:UnequipTools()
    stopDupe = false
    local Pos = plr.Character.HumanoidRootPart.CFrame
    plr.Character:MoveTo(Vector3.new(0, 1e7, 0))
    wait(.15)
    plr.Character.HumanoidRootPart.Anchored = true
    for i = 1, amount do
        if stopDupe then
            break
        end
        wait(0.4)
        plr.Character:MoveTo(Vector3.new(0, 1e7, 0))
        wait(.2)
        for i, v in pairs(plr.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.Name == radio then
                v.Parent = plr.Character
                wait(.08)
                v.Parent = workspace
                table.insert(DropT, v)
            end
        end
        wait(.2)
        Bd()
        repeat 
            game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(";respawn ") wait()
        until plr.CharacterAdded:wait()
        if stopDupe then
            break
        end
    end
    wait(0.2)
    plr.Character.Humanoid:UnequipTools()
    wait(0.3)
    for i, v in pairs(Workspace:GetChildren("\216\167\217\132\216\167\216\186\216\167\217\134\217\138 \217\133\216\179\216\172\217\132")) do
    if v:IsA("Tool") and v.Name == radio then
        firetouchinterest(v.Handle, plr.Character.RightHand, 0)
        plr.Character.HumanoidRootPart.CFrame = Pos
    end
    end
    grabtools()
    wait(0.5)
    ungrabtools()
wait(.4)
end
end    

SpecialPlayerCases = {
	["all"] = function(plr) return Players:GetPlayers() end,
	["others"] = function(plr)
		local plrs = {}
		for i,v in pairs(Players:GetPlayers()) do
			if v ~= plr then
				table.insert(plrs,v)
			end
		end
		return plrs
	end,
	["me"] = function(plr)return {plr} end,
	["#(%d+)"] = function(plr,args,currentList)
		local returns = {}
		local randAmount = tonumber(args[1])
		local players = {unpack(currentList)}
		for i = 1,randAmount do
			if #players == 0 then break end
			local randIndex = math.random(1,#players)
			table.insert(returns,players[randIndex])
			table.remove(players,randIndex)
		end
		return returns
	end,
	["random"] = function(plr,args,currentList)
		local players = Players:GetPlayers()
		local localplayer = Players.LocalPlayer
		table.remove(players, table.find(players, localplayer))
		return {players[math.random(1,#players)]}
	end,
}
