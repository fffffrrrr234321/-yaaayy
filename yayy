local flyjump 
wait()
local Toggle = LP:CreateToggle({
    Name = "flyjump / قفز لانهائي",
    CurrentValue = false,
    Flag = "fj", 
    Callback = function(infjump)
        if infjump == true then
            GUI:Notify({
                 Title= "Toggle",
                Content = "Infjump is Active!",
                Duration = 5.2,
            })
        if flyjump then flyjump:Disconnect() end
        flyjump = UserInputService.JumpRequest:Connect(function(Jump)
            Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
        end)
    else
        GUI:Notify({
             Title= "Toggle",
            Content = "Infjump is Disblade..",
            Duration = 5.2,
        })
        if flyjump then flyjump:Disconnect() end
    end
    end,
})

local homeToggle2 = LP:CreateToggle({
	Name = "Noclip / العبور بين الاشياء",
	CurrentValue = false,
	Flag = "nc", 
	Callback = function(nc)
        if nc == true then
        Clip = false
        wait()
        RunService = game:GetService("RunService")
        local Noclipping = nil
        Clip = false
        wait(0.1)
        local function NoclipLoop()
            if Clip == false and plr.Character ~= nil then
                for _, child in pairs(plr.Character:GetDescendants()) do
                    if child:IsA("BasePart") and child.CanCollide == true and child.Name ~= floatName then
                        child.CanCollide = false
                    end
                end
            end
        end
        Noclipping = RunService.Stepped:Connect(NoclipLoop)
    else
        Clip = true
    end
	end,
})
_G.speedConstant = 200 
local org = nil
local Button = LP:CreateButton({
    Name = "Safe zone / منطقه امنه",
    Callback = function()
        org = plr.Character.HumanoidRootPart.CFrame
        wait(0.05)
        Bd()
        local targetPos = CFrame.new(186.28096, 237.706467, -47.1299744, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        local distance = (plr.Character.HumanoidRootPart.Position - targetPos.Position).Magnitude
        local time = distance / _G.speedConstant

        local tween = TweenService:Create(plr.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear), {
            CFrame = targetPos
        })
        tween:Play()
        plr.Character.Animate.Disabled = true
        tween.Completed:wait()
        plr.Character.HumanoidRootPart.CFrame = CFrame.new(186.28096, 237.706467, -47.1299744, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        plr.Character.Animate.Disabled = false
    end,
})

local Burrrrtton = LP:CreateButton({
    Name = "Teleport back up / الطلوع من المنطقه الامنه",
    Callback = function()
            
    Bd()
        if org == nil then
            local targetPos = CFrame.new(197.672119, 257.515106, -56.6765556, -0.959271967, -3.11977694e-10, 0.282484114, 8.09665182e-11, 1, 1.37935763e-09, -0.282484114, 1.34605083e-09, -0.959271967)
            local distance = (plr.Character.HumanoidRootPart.Position - targetPos.Position).Magnitude
            local time = distance / _G.speedConstant

            local tween = TweenService:Create(plr.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear), {
                CFrame = targetPos
            })
            tween:Play()
            plr.Character.Animate.Disabled = true
            tween.Completed:wait()
            plr.Character.HumanoidRootPart.CFrame = CFrame.new(197.672119, 257.515106, -56.6765556, -0.959271967, -3.11977694e-10, 0.282484114, 8.09665182e-11, 1, 1.37935763e-09, -0.282484114, 1.34605083e-09, -0.959271967)
            plr.Character.Animate.Disabled = false
        else
            local targetPos = org
            local distance = (plr.Character.HumanoidRootPart.Position - targetPos.Position).Magnitude
            local time = distance / _G.speedConstant

            local tween = TweenService:Create(plr.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear), {
                CFrame = targetPos
            })
            tween:Play()
            plr.Character.Animate.Disabled = true
            tween.Completed:wait()
            plr.Character.HumanoidRootPart.CFrame = org
            plr.Character.Animate.Disabled = false 
        end
    end,
})

