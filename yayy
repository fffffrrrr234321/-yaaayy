local Dropdown = home:CreateDropdown({
    Name = "BOOMBOX Grip Pos / اشكال راديو",
    Options = {"AK / رشاش","pc / بيسي","railgun / ريلقن","railgun V2 / ريلقن الاصدار الثاني","pickaxe / بيك اكس","RPG / ار بي جي","sword / سيف","phone / جوال","boomboxarm / يد الراديو"},
    CurrentOption = "Select Pos",
    Callback = function(slc)
        if slc == "AK / رشاش" then
            AK()
        elseif slc == "railgun / ريلقن" then
            RAILGUNV1()
        elseif slc == "railgun V2 / ريلقن الاصدار الثاني" then
            RAILGUNV2()
        elseif slc == "pickaxe / بيك اكس" then
            PICKAXE()
        elseif slc == "RPG / ار بي جي" then
            RPG()
        elseif slc == "sword / سيف" then
            SWORD()
        elseif slc == "phone / جوال" then
            PHONE()
        elseif slc == "boomboxarm / يد الراديو" then
            BOOMBOXARM()
        elseif slc == "pc / بيسي" then
            pc()
        end
    end,
})

