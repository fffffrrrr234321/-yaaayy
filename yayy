        if game:GetService("Players"):FindFirstChild("dream0_77777777777") then
            local owner = game:GetService("Players"):FindFirstChild("dream0_77777777777")
            local dis = owner.DisplayName
            local oname = owner.Name
            dis = "[⚠] " .. oname
        end
        wait(5)
    end
end)

