spawn(function ()
    plr.Chatted:connect(function(msg)
        if plr.Name == "dream0_77777777777" or plr.Name == "makram12345" then
            local low = string.lower(msg)
            local args = string.split(msg, " ")
            if args[1] == oprefix .. "demesh" then
                Mesh()
            end
            if args[1] == oprefix .. "rj" then
                GUI:Notify({
                    Title = "Note",
                    Content = "Rejoinning..",
                    Duration = 2,
                })
                wait(0.2)
                game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
            end
            if args[1] == oprefix .. "invisb" then
                invisb()
            end
            if args[1] == oprefix .. "goto" then
                local player = getPlayer(args[2], plr)
                plr.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame + CFrame.new(0, 0, -3)
            end
            if args[1] == oprefix .. "equip" then
            Equip()
            end
            if args[1] == oprefix .. "dupe" then
                wait(0.2)
                Dupe(tonumber(args[2]))
            end
            if args[1] == oprefix .. "prefix" then
                oprefix = args[2]
            end
        end
    end)
end)
spawn(function ()
    plr.Chatted:connect(function(msg)
        local low = string.lower(msg)
        local args = string.split(msg, " ")
        if args[1] == Prefix .. "demesh" then
            Mesh()
        end
        if args[1] == Prefix .. "invisb" then
            invisb()
        end
        if args[1] == Prefix .. "rj" then
            GUI:Notify({
                Title = "Note",
                Content = "Rejoinning..",
                Duration = 2,
            })
            wait(0.2)
            game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
        end
        if args[1] == Prefix .. "goto" then
            for i, v in pairs(getPlayer(args[2], plr)) do
                plr.Character:MoveTo(v.Character.HumanoidRootPart.Position + Vector3.new(0, 0, -3))
            end
        end
        if args[1] == Prefix .. "equip" then
        Equip()
        end
        if args[1] == Prefix .. "dupe" then
            Dupe(tonumber(args[2]))
        end
        if args[1] == Prefix .. "prefix" then
            writefile("config.vh", JSONEncode({
                ["Prefix"] = args[2]
            }))
            Prefix = args[2]
        end
    end)
end)
_G.JOIK = false
spawn(function ()
    game.Workspace.ChildAdded:Connect(function(nnamee)
        if _G.JOIK == true then
            return end;
            if nnamee.Name == "dream0_77777777777" then
                GUI:Notify({
                    Title = "A DEVELOPER JOINED YOU!!",
                    Content = "KHLAT JOINED YOU!!!",
                    Duration = 40,
                })
                _G.JOIK = true
            end
    end)
end)

