function Equip()
    Bd()
    for i, v in pairs(plr.Backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name == radio then
            wait(.08)
            v.Parent = plr.Character
        end
    end
end

function invisb()
    Bd()
    Equip()
    -- [ Locals ] --
    local Player = game.Players.LocalPlayer
    local Char = Player.Character
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name == "Union" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Beam" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "B1" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "B2" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "BB1" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "BB2" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "ParticleEmitter" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail0" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail1" then
            v:Destroy()
     end
    end
    end

