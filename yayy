            if game:GetService("CoreGui"):WaitForChild("AntiAfk").Enabled == true then
            game:GetService("CoreGui"):WaitForChild("AntiAfk").Enabled = false
            game:GetService("CoreGui"):WaitForChild("AntiAfk").Name = randomstring()
            end
            GUI:Notify({
                Title = "Note",
